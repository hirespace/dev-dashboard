<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hire Space - Dashboard</title>
    <link rel="stylesheet" href="/css/foundation.min.css">
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <header>
        <div class="row">
            <img class="right" src="../images/logo.png" alt="Hire Space Logo" />
            <h1>hirespace <i>dev dashboard</i></h1>
        </div>
    </header>
    <section class="main-content">
        <div class="row">
            <div class="large-4 columns text-center" data-role="stat-container" data-id="hirespacecom">
                <div class="light offset-left">
                    <div class="site-name">hirespace</div>
                    <span data-role="status">Waiting...</span>
                </div>
                <div id="lastRun" class="text-center offset-left" data-role="last-run">Not yet run</div>
            </div>
            <div class="large-4 columns text-center" data-role="stat-container" data-id="venuescom">
                <div class="light offset-left">
                    <div class="site-name">bookings</div>
                    <span data-role="status">Waiting...</span>
                </div>
                <div id="lastRunVenues" class="text-center offset-left" data-role="last-run">Not yet run</div>
            </div>
            <div class="large-4 columns text-center" data-role="stat-container" data-id="search">
                <div class="light offset-left">
                    <div class="site-name">search</div>
                    <span data-role="status">Waiting...</span>
                </div>
                <div id="lastRunSearch" class="text-center offset-left" data-role="last-run">Not yet run</div>
            </div>
        </div>
    </section>

    <script src="../Scripts/vendor/jquery.min.js" type="text/javascript"></script>
    <script src="http://js.pusher.com/2.1/pusher.min.js" type="text/javascript"></script>
    <script src="../Scripts/dashboard.js" type="text/javascript"></script>

</body>
</html>